<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nju.edu.erp.dao.WarehouseInputDao">
    <insert id="save" parameterType="WarehouseInputPO">
        insert into warehouse_input (id, batch_id, operator, update_time)
        values (#{id},#{batchId},#{operator},#{updateTime})
    </insert>

    <select id="getLatest" resultMap="WarehouseInputPO">
        select * from warehouse_input order by id desc limit 0,1 ;
    </select>

    <resultMap id="WarehouseInputPO" type="com.nju.edu.erp.model.po.WarehouseInputPO">
        <id column="id" property="id"></id>
        <result column="batch_id" property="batchId"></result>
        <result column="operator" property="operator"></result>
        <result column="update_time" property="updateTime"></result>
    </resultMap>
</mapper>